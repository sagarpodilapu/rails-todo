<%= render partial:"includes/header" %>
<% if flash[:notice] %>
  <div class='notice'><%= flash[:notice]%></div>
<% end %>
<% if flash[:success] %>
  <div class='success'><%= flash[:success]%></div>
<% end %>
<% if flash[:danger] %>
  <div class='danger'><%= flash[:danger]%></div>
<% end %>

<%= yield %>

<%= render partial:"includes/footer" %>
